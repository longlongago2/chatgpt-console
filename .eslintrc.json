{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "linebreak-style": ["warn", "windows"],
    "no-console": "off",
    "import/extensions": ["error", "always"],
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": "off"
  }
}
